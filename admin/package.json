{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-testing": "^3.1.3",
    "apollo-boost": "^0.4.7",
    "autoprefixer": "^9.7.6",
    "axios": "^0.19.0",
    "babel-plugin-macros": "^2.8.0",
    "classnames": "^2.2.6",
    "flexboxgrid": "^6.3.1",
    "graphql": "^14.6.0",
    "lodash": "^4.17.15",
    "postcss-cli": "^7.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "styled-components": "^5.1.0",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.3.5"
  },
  "scripts": {
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
    "watch:css": "postcss src//styles/tailwind.css -o src/styles/main.css",
    "start": "npm run watch:css & react-scripts start",
    "build": "npm run build:css react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen --colors",
    "test:coverage": "react-scripts test --env=jest-environment-jsdom-sixteen --coverage --watchAll=false --silent --colors"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "apollo-link-mock": "^1.0.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!**/*.{tw.js,config.js}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
